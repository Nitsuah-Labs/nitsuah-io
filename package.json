{
  "name": "next-connectkit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:wagmi": "npm run wagmi && next dev",
    "build": "npm run wagmi && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "wagmi": "node scripts/wagmi-generate.js",
    "typecheck": "tsc --project config/tsconfig.json --noEmit",
    "format": "prettier --config config/prettier.config.json --write \"src/**/*.{js,jsx,ts,tsx,json,css}\"",
    "format:check": "prettier --config config/prettier.config.json --check \"src/**/*.{js,jsx,ts,tsx,json,css}\"",
    "test": "jest --config config/jest.config.ts",
    "test:watch": "jest --config config/jest.config.ts --watch",
    "test:coverage": "jest --config config/jest.config.ts --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "validate": "npm run typecheck && npm run format:check",
    "clean": "rm -rf .next out node_modules/.cache",
    "analyze": "cross-env ANALYZE=true npm run build",
    "prepare": "husky"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css}": [
      "prettier --config config/prettier.config.json --write"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.14",
    "@mui/material": "^5.14.14",
    "@splinetool/react-spline": "^2.2.6",
    "@splinetool/runtime": "^0.9.486",
    "@tanstack/react-query": "^5.90.5",
    "bufferutil": "^4.0.9",
    "encoding": "^0.1.13",
    "lokijs": "^1.5.12",
    "next": "^14.2.26",
    "pino-pretty": "^13.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.34.1",
    "utf-8-validate": "^6.0.5",
    "viem": "^2.38.4",
    "wagmi": "^2.18.2"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@eslint/js": "^9.26.0",
    "@playwright/test": "^1.49.1",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/aria-query": "^5.0.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^17.0.31",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@wagmi/cli": "^2.7.1",
    "babel-jest": "^30.2.0",
    "dotenv": "^16.0.0",
    "esbuild": "^0.23.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.3.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^14.0.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.1.6",
    "typescript-eslint": "^8.31.0"
  }
}
