{
  "name": "next-connectkit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:wagmi": "npm run wagmi && next dev",
    "build": "npm run wagmi && next build",
    "build:skip-wagmi": "next build",
    "build:ci": "npm run build:skip-wagmi",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "wagmi": "node scripts/wagmi-generate.js",
    "typecheck": "tsc --project config/tsconfig.json --noEmit",
    "format": "prettier --config config/prettier.config.json --write \"src/**/*.{js,jsx,ts,tsx,json,css}\"",
    "format:check": "prettier --config config/prettier.config.json --check \"src/**/*.{js,jsx,ts,tsx,json,css}\"",
    "test": "jest --config config/jest.config.ts",
    "test:watch": "jest --config config/jest.config.ts --watch",
    "test:coverage": "jest --config config/jest.config.ts --coverage",
    "test:e2e": "cross-env NEXT_PUBLIC_TEST_HELPERS=1 playwright test",
    "test:e2e:ci": "cross-env NEXT_PUBLIC_TEST_HELPERS=1 playwright test",
    "test:e2e:ui": "cross-env NEXT_PUBLIC_TEST_HELPERS=1 playwright test --ui",
    "test:e2e:headed": "cross-env NEXT_PUBLIC_TEST_HELPERS=1 playwright test --headed",
    "test:e2e:docker": "docker-compose -f docker-compose.test.yml run --rm playwright",
    "test:e2e:docker:build": "docker-compose -f docker-compose.test.yml build",
    "test:visual": "playwright test tests/visual --project=chromium-desktop",
    "test:visual:update": "bash scripts/update-visual-baselines.sh --local",
    "test:visual:update:docker": "bash scripts/update-visual-baselines.sh --docker",
    "test:a11y": "playwright test tests/accessibility --project=chromium-desktop",
    "test:a11y:quick": "playwright test tests/accessibility/all-pages.spec.ts --grep=\"(Homepage|About|Projects)\" --project=chromium-desktop",
    "test:smoke": "playwright test tests/accessibility/all-pages.spec.ts --grep=\"Homepage has no accessibility violations\" --project=chromium-desktop --timeout=5000",
    "test:precommit": "npm run test:smoke",
    "validate": "npm run typecheck && npm run format:check",
    "validate:full": "npm run validate && npm run lint && npm run test:a11y:quick",
    "commit:quick": "git commit --no-verify",
    "commit:safe": "npm run typecheck && npm run format && git add -A && git commit",
    "clean": "rm -rf .next out node_modules/.cache",
    "analyze": "cross-env ANALYZE=true npm run build",
    "prepare": "husky"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --config config/prettier.config.json --write",
      "eslint --fix"
    ],
    "src/**/*.{json,css}": [
      "prettier --config config/prettier.config.json --write"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.14.1",
    "@metamask/sdk": "^0.34.0",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "@safe-global/safe-apps-provider": "^0.18.6",
    "@safe-global/safe-apps-sdk": "^9.1.0",
    "@splinetool/react-spline": "^4.1.0",
    "@splinetool/runtime": "^1.12.13",
    "@tanstack/react-query": "^5.90.12",
    "@wagmi/connectors": "^7.0.2",
    "@wagmi/core": "^3.0.0",
    "@walletconnect/ethereum-provider": "^2.23.1",
    "bufferutil": "^4.0.9",
    "next": "^16.0.10",
    "react": "^19.2.1",
    "react-dom": "^19.2.3",
    "react-hot-toast": "^2.6.0",
    "sharp": "^0.34.1",
    "utf-8-validate": "^6.0.5",
    "viem": "^2.41.2",
    "wagmi": "^3.1.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@eslint/js": "^9.26.0",
    "@playwright/test": "^1.56.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/aria-query": "^5.0.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.49.0",
    "@wagmi/cli": "^2.8.0",
    "babel-jest": "^30.2.0",
    "baseline-browser-mapping": "^2.9.6",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "esbuild": "^0.27.1",
    "eslint": "^9.39.0",
    "eslint-config-next": "^16.0.8",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.2.6",
    "prettier": "^3.7.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0"
  }
}
