+const mockMenu = [
+  {
+    name: "Starters",
+    items: [
+      {
+        name: "Bruschetta",
+        emoji: "üçû",
+        price: "$6",
+        description: "Toasted bread with tomatoes",
+      },
+    ],
+  },
+  {
+    name: "Mains",
+    items: [
+      {
+        name: "Lasagna",
+        emoji: "üçù",
+        price: "$14",
+        description: "Layers of pasta and cheese",
+      },
+    ],
+  },
