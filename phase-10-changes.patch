+            padding: showDemo ? "0.5rem 1rem" : "0.75rem 1.5rem",
+            borderRadius: "8px",
+            border:
+              selectedType === type
+                ? `2px solid ${color}`
+                : "2px solid rgba(255, 255, 255, 0.2)",
